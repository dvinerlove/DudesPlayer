<UserControl
    x:Class="DudesPlayer.Desktop.Views.Pages.RoomsExplorerPage.Dialogs.ConnectToRoomDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DudesPlayer.Desktop.Views.Pages.RoomsExplorerPage.Dialogs"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Width="300"
    Height="auto"
    mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <ui:TextBox Name="RoomName" PlaceholderText="Room name" />
            <ui:TextBox Margin="0,8,0,0" PlaceholderText="Room password" />

        </StackPanel>
    </Grid>
</UserControl>
